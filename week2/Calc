package saranya.molekule.Test1;

public class Cal {
	public void add() {
		int a = 5 ,b = 10;
		System.out.println("Sum:" + (a+b));
	}
	
	public void add (int a, int b) {
		System.out.println("Sum:"+	(a+b));
	}
	public void add (int a, int b,int c ) {
		System.out.println("Sum:"+	(a+b+c));
		
		
	}
	
	public void add (double a,double b) {
		System.out.println("Sum:"+(a+b));
	}
}
//the abve has so many methods so Method overloading.Alowe inside only same class
