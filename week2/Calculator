package saranya.molekule.Test1;

public class Calculator {
	public int addTwoNumber(int a,int b) {
		
		int sum = (a+b);{
 
		return sum;
		
		
		}
		
		 
		
		
}

public static void main(String[] args) {
	Calculator answer = new Calculator();
	
	int result = answer.addTwoNumber(500,432);
	int result1 = answer.addTwoNumber(45, 32);
	int result2= answer.addTwoNumber(3235, 23546);
	
	
	System.out.println(result+","+result1+","+result2);
	

		
		
}



}
